@import url(https://fonts.googleapis.com/css?family=Lato);

body
  margin: 0
  padding: 120px 0 0 0
  background-image: url("images/starfield.png")
  background-position: center
  background-size: cover
  background-repeat: no-repeat
  background-color: #161616

h1
  font-family: Bebas Neue
  font-size: 48px
  font-weight: normal
  color: white

h3
  font-family: Lato
  font-size: 18px
  color: rgb(208, 208, 208)

h4
  font-family: Lato
  font-size: 16px
  color: rgb(180, 180, 180)

h5
  font-family: Bebas Neue
  font-size: 16px
  font-weight: normal
  color: rgb(170, 170, 170)

p
  font-family: Lato
  font-size: 12px
  color: rgb(164, 164, 164)

.container
  max-width: 1000px
  margin-left: auto
  margin-right: auto
  //height:100%
  display: -webkit-box
  display: -moz-box
  display: -ms-flexbox
  display: -moz-flex
  display: -webkit-flex
  display: flex
  flex-direction:row
  flex-wrap:wrap
  align-items: flex-start
  -webkit-flex-direction:row
  -webkit-flex-wrap:wrap
  -webkit-align-items: flex-start
  padding:5px

.center
  justify-content: center
  -webkit-justify-content: center

.right
  justify-content: flex-start
  -webkit-justify-content: flex-start

.grid-1
  width: 100px

.grid-2
  width: 200px

.grid-3
  width: 300px

.grid-4
  width: 400px

.grid-5
  width: 500px

.grid-6
  width: 600px

.grid-7
  width: 700px

.grid-8
  width: 800px

.grid-9
  width: 900px

.grid-10
  width: 1000px

.label-list h5
  display:block
  line-height: 35px
  margin-top: 10px
  margin-bottom: 0

.label-list p
  display: inline

.label
  padding: 3px 5px 3px 5px
  background-color: rgb(41, 186, 173)
  border-radius: 2px
  color: rgb(18, 93, 86)

#title h1
  margin-bottom: 0

#title h4
  margin-top: 0
